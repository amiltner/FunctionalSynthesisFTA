fix (f : list -> list) =
  fun (x:list) ->
    match x binding _ with
      | Nil -> x
      | Cons -> (match Un_Cons x . 1 binding _ with
                   | Nil -> x
                   | Cons -> (match compare (Un_Cons x . 0)
                                      (Un_Cons (Un_Cons x . 1) . 0) binding _ with
                                | LT -> Cons (Un_Cons x . 0,
                                               f (Un_Cons x . 1))
                                | EQ -> f (Un_Cons x . 1)
                                | GT -> Cons (Un_Cons x . 0,
                                               f (Un_Cons x . 1))))