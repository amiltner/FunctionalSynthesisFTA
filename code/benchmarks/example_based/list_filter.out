fix (f : (nat -> bool) * list -> list) =
  fun (x:(nat -> bool) * list) ->
    match x . 1 binding _ with
      | Nil -> x . 1
      | Cons -> (match x . 0 (Un_Cons (x . 1) . 0) binding _ with
                   | True -> Cons (Un_Cons (x . 1) . 0,
                                    f (x . 0, Un_Cons (x . 1) . 1))
                   | False -> Un_Cons (x . 1) . 1)