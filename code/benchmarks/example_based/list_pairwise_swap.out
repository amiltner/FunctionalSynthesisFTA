fix (f : list -> list) =
  fun (x:list) ->
    match x binding _ with
      | Nil -> x
      | Cons -> (match Un_Cons x . 1 binding _ with
                   | Nil -> Un_Cons x . 1
                   | Cons -> (match f (Un_Cons x . 1) binding _ with
                                | Nil -> Cons (Un_Cons (Un_Cons x . 1) . 0,
                                                Cons (Un_Cons x . 0,
                                                       f
                                                         (Un_Cons (Un_Cons x
                                                                    . 1)
                                                            . 1)))
                                | Cons -> Nil))