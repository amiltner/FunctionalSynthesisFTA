include "bool.decls"

synth bool -> bool -> bool satisfying

let bool_and =
  fun (b1:bool) ->
    fun (b2:bool) ->
      match b1 with
      | True -> b2
      | False -> False
;;

fun (in1in2:bool*bool) -> (fun (out : bool) ->
    match in1in2.0 with
    | True -> out == in1in2.1
    | False -> out == False)
