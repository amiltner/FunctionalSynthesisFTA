include "nat.decls"

synth nat -> bool satisfying

fun (input:nat) -> (fun (out : bool) ->
    match input with
    | S O -> out == False
    | S S O -> out == True
    | S S S O -> out == False
    | _ -> True)

(*
    | O -> out == True
[0] -> True,
[1] -> False,
[2] -> True,
[3] -> False,
[4] -> True
*)
